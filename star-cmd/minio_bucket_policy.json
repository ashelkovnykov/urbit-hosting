{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Sid": "AllowPublicRead",
          "Effect": "Allow",
          "Principal": {
              "AWS": [
                  "*"
              ]
          },
          "Action": [
              "s3:GetObject"
          ],
          "Resource": [
              "arn:aws:s3:::[BUCKET_NAME]/*"
          ]
      },
      {
          "Sid": "DenyPublicWrite",
          "Effect": "Deny",
          "Principal": {
              "AWS": [
                  "*"
              ]
          },
          "Action": [
              "s3:PutObject"
          ],
          "Resource": [
              "arn:aws:s3:::[BUCKET_NAME]/*"
          ]
      }
  ]
}
